{
  "name": "todoListApp",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel watch index.html",
    "build": "rm -rf dist && babel src -d dist --presets=es2015",
    "dev-demon": "nodemon ./scripts/scripts.js",
    "watch": "concurrently \"npm run serve\" \"babel src -d dist --preset=es2015 -ws\"",
    "serve": "http-server"
  },
  "dependencies": {
    "bootstrap": "^4.1.3"
  },
  "devDependencies": {
    "boostrap": "^2.0.0",
    "parcel-bundler": "^1.10.3",
    "nodemon": "^1.18.9",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1"
  }
}
